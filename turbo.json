{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "src:gen": {
      "dependsOn": ["^src:gen"],
      "outputs": ["dist/**"]
    },
    "build": {
      "dependsOn": ["^src:gen", "^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "type:check": {
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^src:gen"]
    }
  }
}
