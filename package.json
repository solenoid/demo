{
  "name": "demo",
  "private": true,
  "license": "ISC",
  "version": "0.0.0",
  "description": "Turbo Mono Repo Explorations.",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "clean:check": "git clean -xdn",
    "clean:self": "npm exec -y -- rimraf@3.0.2 node_modules",
    "clean:extra": "npm exec -y -- rimraf@3.0.2 apps/home/.vscode apps/**/node_modules packages/**/node_modules",
    "type:check": "turbo run type:check",
    "format": "prettier --write  '**/*.{js,json,ts,tsx,md}'",
    "format:check": "prettier -c '**/*.{js,json,ts,tsx,md}'"
  },
  "dependencies": {},
  "devDependencies": {
    "@solenoid/eslint-config": "*",
    "prettier": "2.8.0",
    "turbo": "1.6.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@8.19.2",
  "volta": {
    "node": "18.12.1"
  }
}
