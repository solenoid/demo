{
  "name": "demo",
  "private": true,
  "license": "ISC",
  "version": "0.0.0",
  "description": "Nx and pnpm Mono Repo Explorations.",
  "scripts": {
    "build": "nx run-many --target=build",
    "dev": "nx run-many --target=dev",
    "lint": "nx run-many --target=lint",
    "type:check": "nx run-many --target=type:check",
    "clean": "nx run-many --target=clean",
    "format": "prettier --write '**/*.{js,json,ts,tsx,md}'",
    "format:check": "prettier -c '**/*.{js,json,ts,tsx,md}'",
    "clean:all": "pnpm clean && pnpm clean:extra && pnpm clean:self",
    "clean:check": "git clean -xdn",
    "clean:extra": "rimraf apps/home-nextjs/.vscode apps/**/node_modules packages/**/node_modules",
    "clean:self": "rimraf node_modules"
  },
  "devDependencies": {
    "@solenoid/eslint-config": "*",
    "nx": "15.3.3",
    "prettier": "2.8.1"
  },
  "packageManager": "pnpm@7.18.2",
  "engines": {
    "node": "18"
  }
}
